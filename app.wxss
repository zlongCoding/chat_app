/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
::-webkit-scrollbar {
  display: none;
}
page {
  -webkit-font-smoothing: antialiased;
  font-family: "PingHei","Helvetica Neue","Helvetica","Arial","Verdana","sans-serif";
  font-size: 28rpx;
}
.hoverClass,
.navigator-hover {
  background: #f2f2f2 !important;
}
swiper.swiper {
  flex: 1;
}
.swiper swiper-item {
  overflow: scroll;
}
.swiper scroll-view {
  height: 100%;
}

.page_content{
   min-height: 100vh;
}
